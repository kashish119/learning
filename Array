// Second largest
#include <bits/stdc++.h> 
int findSecondLargest(int n, vector<int> &arr)
{
    int max = arr[0] , secmax = INT_MIN;
    for( int i=1 ; i<n ; i++ ){
        if ( arr[i] > max ){ 
              secmax = max;
              max = arr[i];
          }
         else if( arr[i] < max && arr[i] > secmax)
           secmax = arr[i];
    }
    if ( secmax == INT_MIN)
     secmax = -1;
    return secmax;
}
